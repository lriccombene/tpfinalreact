h(\"/\")\n          }else{\n            this.setState({\n              alert:{\n                variant:\"danger\",\n                text:\"ERROR\"\n              }\n            })\n          }\n        })\n    e.preventDefault()\n  }\n\n\n\n  componentDidMount() {\n    if (this.state.categoria._id) {\n      Api.getCategorias(this.state.categoria._id)\n        .then(response => {\n          const [error, data] = response\n          if (error) {\n            this.setState({\n              errors: data\n            })\n          } else {\n            this.setState({\n              categoria: data,\n              errors: []\n            })\n          }\n        })\n    }\n  }\n\n  render() {\n    const { redirect, categoria, errors } = this.state\n\n    if (redirect) {\n      return (\n        <Redirect to={redirect} />\n      )\n    } else {\n\n      return (\n        <Container>\n          <Row>\n            <Col>\n              <h3>Editar categoria</h3>\n\n              {errors.length > 0 &&\n                <div>\n                  {errors.map((error, index) =>\n                    <Alert color=\"danger\" key={index}>\n                      {error}\n                    </Alert>\n                  )}\n                </div>\n              }\n\n              <Form onSubmit={this.handleSubmit}>\n                <FormGroup>\n                  <Label for=\"name\">Nombre</Label>\n                  <Input type=\"text\" name=\"name\" id=\"name\" value={categoria.name} placeholder=\"Ingrese nombre\" onChange={this.setName} />\n                </FormGroup>\n\n                <Button color=\"success\">Guardar</Button>\n              </Form>\n\n            </Col>\n          </Row>\n          <AlertCustom variant={this.state.alert.variant} text={this.state.alert.text} />\n        </Container>\n      )\n    }\n  }\n}\n\nexport default CategoriasForm\n","/home/tatilumate/Documentos/curso-utn-webavanzado /proyectofinalreact/tpfinalreact/tpfinalreact/src/Components/Categorias/Categ